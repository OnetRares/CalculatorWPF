<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Row0Button">#FCE4EC</SolidColorBrush>
    <SolidColorBrush x:Key="Row1Button">#F8BBD0</SolidColorBrush>
    <SolidColorBrush x:Key="Row2Button">#F48FB1</SolidColorBrush>
    <SolidColorBrush x:Key="Row3Button">#F06292</SolidColorBrush>
    <SolidColorBrush x:Key="Row4Button">#EC407A</SolidColorBrush>
    <SolidColorBrush x:Key="Row5Button">#E91E63</SolidColorBrush>
    <SolidColorBrush x:Key="Row6Button">#D81B60</SolidColorBrush>

    <LinearGradientBrush x:Key="AppBackground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FCE4EC"/>
        <GradientStop Offset="1.0" Color="#F48FB1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ButtonBackground" Color="#F06292"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="White"/>

</ResourceDictionary>
