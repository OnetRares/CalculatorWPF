<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Row0Button">#FFF9C4</SolidColorBrush>
    <SolidColorBrush x:Key="Row1Button">#FFF59D</SolidColorBrush>
    <SolidColorBrush x:Key="Row2Button">#FFF176</SolidColorBrush>
    <SolidColorBrush x:Key="Row3Button">#FFEB3B</SolidColorBrush>
    <SolidColorBrush x:Key="Row4Button">#FDD835</SolidColorBrush>
    <SolidColorBrush x:Key="Row5Button">#FBC02D</SolidColorBrush>
    <SolidColorBrush x:Key="Row6Button">#F9A825</SolidColorBrush>

    <LinearGradientBrush x:Key="AppBackground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#FFF9C4"/>
        <GradientStop Offset="1.0" Color="#FFEB3B"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ButtonBackground" Color="#FFEB3B"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="Black"/>
</ResourceDictionary>
